{
  "name": "ripple",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build --prod",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "depoly": "npm run build && gh-pages -d www"
  },
  "devDependencies": {
    "@stencil/core": "^2.4.0",
    "@stencil/sass": "1.3.2",
    "gh-pages": "^3.1.0"
  },
  "license": "MIT"
}
